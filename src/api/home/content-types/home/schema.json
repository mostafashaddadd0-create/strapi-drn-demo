    "blogTitle": {
      "type": "string"
    },
    "blogCtaLabel": {
      "type": "string"
    },
    "blogCtaUrl": {
      "type": "string"
    },
    "blogPosts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog"
    },
    "brands": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
