      "type": "text"
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service"
    },
    "stepsTitle": {
      "type": "text"
    },
    "stepsVideoUrl": {
      "type": "string"
    },
